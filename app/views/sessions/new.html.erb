<h1>Sign In</h1>
<% if flash[:alert] %>
	<%= flash[:alert] %>
<% end %>
<%= form_with url: "sessions/create" do |f| %>
	<div>
		<%= f.label :email %>
		<%= f.text_field :email %>
	</div>
	<div>
		<%= f.label :password %>
		<%= f.password_field :password %>
	</div>
	<div>
		<%= f.submit %>
	</div>
<% end %>
